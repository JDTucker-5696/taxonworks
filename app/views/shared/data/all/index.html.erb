<div class="flexbox" > 
  <div class="item item1">
    <div class="right_float">
      <span class="subtle"><%= favorite_page_link('data', @data_model.name) -%></span>
    </div>
  </div>
</div>
<div class="flexbox" id="model_index">
  <div class="item item1">
    <h1><img onClick="alert('Not working yet.')" src="/assets/icons/empty_star.svg" title="Click here to add the page to your favorite pages" class="icon-fav-star"/><%= model_name_title -%></h1>
  </div>  
  <div class="item item2">
    <%= search_form_for_model(@data_model) %>
  </div>  
  <div class="item item3 icon-new">
    <%= new_for_model_link(@data_model)  %>  
  </div>
  <div class="item item4 icon-batch">
    <%= batch_load_link -%>  
  </div>
  <div class="item item5 icon-list">
    <%= list_for_model_link(@data_model) %>
  </div>
  <div class="item item6 icon-download">
    <%= download_for_model_link(@data_model) %>
  </div>
</div>
<div class="flexbox"> 
  <div class="item item1 recent_updates center_text ">
    <h2>Recent updates</h2>
    <%= recent_objects_list(@data_model, @recent_objects) -%>
  </div>
</div>

