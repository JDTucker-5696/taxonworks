<h3> Annotations </h3>
<% if object.annotations.empty? -%>
  <p> None. </p>
<% else %>
  <% object.annotations.each do |title, values| -%>
    <%= content_tag(:h4, title.humanize)  -%>
      <% values.each do |v| -%>
        <%= content_tag(:p, (title == 'depictions' ? object_link(v) : object_tag(v))+ '  ' + link_to('edit', edit_object_path(v)), class: 'list')   -%>
      <% end %>
  <% end %>
<% end %>


