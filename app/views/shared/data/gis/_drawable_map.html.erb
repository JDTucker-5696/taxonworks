<div id="_drawable_map_outer">
  <div id="_drawable_map_form"

       data-feature-collection="<%= Gis::GeoJSON.feature_collection(map_objects).to_json -%>"
       data-map-center='POINT (0.0 0.0 0.0)'
       data-map-canvas="drawable_map_canvas"
  >
  </div>
  <table <%= border_string %>>
    <tbody>
    <tr>
      <td>
        <div id="drawable_map_canvas" style="width: 512px; height: 256px;"></div>
        <span id="map_coords">drawable map Coordinates</span>
      </td>
    </tr>
    </tbody>
  </table>
</div>
