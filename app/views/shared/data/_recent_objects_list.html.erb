<div>
  <h2> Recent Updates </h2>
  <ul> 
    <% @recent_objects.each do |r| -%>
      <li> <%= object_link(r) -%> <%= edit_object_link(r) -%>  <%= object_time_since_update_tag(r) -%> ago </li>
  <% end %>
  </ul>
  <%= content_tag(:em, "No recently updated or created #{@data_model.name.tableize.humanize}.") if @recent_objects.empty? -%>
</div>
