
<table class="tablesorter">

  <thead>
  <tr>


    <th> Name </th>
    <%= fancy_th_tag(name: 'Updated by', group: 'housekeeping') -%>
    <%= fancy_th_tag(name: 'Last updated', group: 'housekeeping') -%>
  </tr>
  </thead>

  <tbody>
  <% otus.each do |o| -%>
    <%= content_tag(:tr, class: [:contextMenuCells, :btn, 'btn-neutral']) do -%>
      <td> <%= o.name %></td>
       <td><%= o.updated_by -%></td>
       <td><%= o.updated_at -%></td>
    <% end %>

  <% end %>

  </tbody>
</table>

