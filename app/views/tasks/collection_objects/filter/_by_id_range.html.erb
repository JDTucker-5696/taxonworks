<%= form_tag(set_id_range_for_collection_object_filter_url, id: 'set_id_range_form', remote: true) do %>
    Namespace (optional):<br>
    <%= namespace_select_tag(:id_namespace) -%><br>
    Range:<br>
    <% @id_pile = CollectionObject.id_group(nil) %>
    Start: <%= id_range_select_tag(:id_range_start, @id_pile) -%>
    Stop: <%= id_range_select_tag(:id_range_stop, @id_pile) -%><br>
    <%= submit_tag('Set Identifier Range', id: 'set_id_range') %>
<% end %>
