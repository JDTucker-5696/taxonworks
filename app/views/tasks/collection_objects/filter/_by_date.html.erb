<h3><span id="date_count">????</span></h3>
<span id="earliest_date" hidden="hidden"><%= @earliest_date %></span>
<span id="latest_date" hidden="hidden"><%= @latest_date %></span>
<%= form_tag(set_date_for_collection_object_filter_path, id: 'set_date_form', remote: true) do %>
  <p>Partial overlap search: <%= check_box nil, 'partial_overlap', {id: :partial_toggle}, 'on', 'off' %></p>
    <div class="flex-separate">
      <div>
        <label for="search_start_date">Start date:</label><br>
        <input type="text" id="search_start_date" name="search_start_date"/>
      </div>
      <div>
        <label for="search_end_date">End date:</label><br>
        <input type="text" id="search_end_date" name="search_end_date" />
      </div>
    </div>
    <br/>
    <div id="tr_slider" class="flex-wrap-column" >
      <div id="double_date_range"></div>
      <div class="horizontal-center-content"><br>
        <input id="reset_slider" type="button" value="Reset Slider"/>
      </div>
    </div>
    <div id="tr_calendar" class="flex-separate">
      <div id="st_fixedpicker"></div>
      <div id="en_fixedpicker"></div>
    </div>
    <div class="horizontal-center-content" id="graph_frame">
      <div><p>Result graph</p></div>
    </div>
<% end %>
