<div id="_result_ce_form">
  <table border="1">
    <tr>
      <th>Georeference</th>
      <th>Result</th>
      <th>CE #</th>
      <!--<th></th>-->
    </tr>
    <tbody>
    <% georeferences_results.each do |g| %>
        <tr>
          <td>
            <%= g.new_record? ? 'not saved' : link_to("show", g.metamorphosize) %>
          </td>
          <td>
            <%= g.new_record? ? false : true %>
          </td>
          <td>
            <%= g.new_record? ? '-' : g.collecting_events.count %>
          </td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
