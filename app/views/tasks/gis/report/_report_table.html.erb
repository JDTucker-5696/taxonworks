<table border="1" align="center">
  <tbody>
  <tr>
    <th>OTU</th>
    <th>OTU name</th>
    <th>Family</th>
    <th>Genus</th>
    <th>Species</th>
    <th>Country</th>
    <th>State</th>
    <th>County</th>
    <th>Locality</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <%= ce_attrib_headers.html_safe %>
    <%= co_attrib_headers.html_safe %>
    <%= bc_headers.html_safe %>
  </tr>
  <% @collection_objects.each do |c_o| %>
      <tr>
        <td><%= get_otu(c_o).id %></td>
        <td><%= get_otu(c_o).name %></td>
        <td><%= name_at_rank_string(c_o, :family) %></td>
        <td><%= name_at_rank_string(c_o, :genus) %></td>
        <td><%= name_at_rank_string(c_o, :species) %></td>
        <% parse_names(c_o) -%>
        <td><%= @geo_names[0] %></td>
        <td><%= @geo_names[1] %></td>
        <td><%= @geo_names[2] %></td>
        <td><%= c_o.collecting_event.verbatim_locality %></td>
      </tr>
  <% end %>
  </tbody>
</table>
