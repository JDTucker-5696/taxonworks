<div id="_tag_ce_partial">
  <%= form_for(@collecting_event) do |f| %>
      Find collecting event tagged with
      <table border="1">
        <tr>
          <div class="field">
            <td><%= f.label 'keyword' %></td>
            <td><%= f.text_field :verbatim_label, size: 35 %></td>
          </div>
        </tr>
        <tr>
          <div class="actions">
            <td></td>
            <td><%= f.submit value: 'Find' %></td>
          </div>
        </tr>
      </table>
  <% end %>
</div>


