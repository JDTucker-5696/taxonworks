<nav id="quick_bar">
  <ul class="horizontal_navbar left_justified">
    <%= content_tag(:li, link_to('hub', hub_path, class: :hub_link)) -%>

    <% if Hub::Data::BY_NAME[@data_model.name] -%> 
      <%= link_to(Hub::Data::BY_NAME[@data_model.name].name, @data_model) -%> 

      <% Hub::Data::BY_NAME[@data_model.name].related_models.each do |m| -%>
        <%= quick_bar_link(m) -%> 
      <% end %>
  <% end %> 
    </ul>
</nav>
