<%= content_tag :h2, user_tag(@user) %>
<p>
  <%= sessions_current_user_id == @user.id ? content_tag(:span, 'You!', class: [:feedback, 'feedback-success']) :
    content_tag(:span, 'NOT YOU!', class: [:feedback, 'feedback-danger']) -%>
</p>

<div class="flexbox">
    <div class="item item1">
      <%= render 'attributes' %>
      <%= link_to 'Edit', edit_user_path(@user) %>
    </div>

  <div class="item item2">
    <%=  (link_to('hub', hub_path) + " for current project #{sessions_current_project.name}")  if sessions_current_project_id -%> <br/>
      <%= link_to('dashboard', dashboard_path) -%>
  </div>
</div>


