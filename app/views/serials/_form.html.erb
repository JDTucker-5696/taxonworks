<%= form_for(@serial) do |f| %>
    <%= render 'shared/errors', object: @serial %>

    <div class="field">
      <%= f.label :name %>
      <br>
      <%= f.text_area :name, :cols => 40, :rows => 3 %>
    </div>

    <div class="field">
      <%= f.label :publisher %>
      <br>
      <%= f.text_field :publisher %>
    </div>
    <div class="field">
      <%= f.label :place_published %>
      <br>
      <%= f.text_field :place_published %>
    </div>
    <div class="field">
      <%= f.label :primary_language_id %>
      <br>
      <%= f.number_field :primary_language_id %>
    </div>
    <div class="field">
      <%= f.label :first_year_of_issue %>
      <br>
      <%= f.number_field :first_year_of_issue %>
    </div>
    <div class="field">
      <%= f.label :last_year_of_issue %>
      <br>
      <%= f.number_field :last_year_of_issue %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
