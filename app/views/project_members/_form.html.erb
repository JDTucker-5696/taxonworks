<%= form_for(@project_member) do |f| %>
    <%= render 'shared/errors', object: @project_member %>

  <div class="field">
    <%= f.label :project_id %><br>
    <%= f.select :project_id, options_from_collection_for_select(Project.all, :id, :name) %>
  </div>
  <div class="field">
    <%= f.label :user_id %><br>
    <%= f.select :user_id, options_from_collection_for_select(User.all, :id, :email) %>
  </div>
  <div class="field">
    <%= f.label :is_project_administrator %><br>
    <%= f.check_box :is_project_administrator %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
