<div class="attributes">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Image:</strong>
    <%= image_tag(@image.image_file.url) %>
  </p>

  <p>
    <strong>User file name:</strong>
    <%= @image.user_file_name %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= @image.height %>
  </p>

  <p>
    <strong>Width:</strong>
    <%= @image.width %>
  </p>

  <p>
    <strong>Image file fingerprint:</strong>
    <%= @image.image_file_fingerprint %>
  </p>

  <p>
    <strong>Image file file name:</strong>
    <%= @image.image_file_file_name %>
  </p>

  <p>
    <strong>Image file content type:</strong>
    <%= @image.image_file_content_type %>
  </p>

  <p>
    <strong>Image file file size:</strong>
    <%= @image.image_file_file_size %>
  </p>

  <p>
    <strong>Image file updated at:</strong>
    <%= @image.image_file_updated_at %>
  </p>

</div>