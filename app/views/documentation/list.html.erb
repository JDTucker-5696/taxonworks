<h1>Listing documentation</h1>

<%= paginate @documentation %> <%= page_entries_info @documentation %>

<table>
  <thead>
    <tr>
      <th>Documentation object</th>
      <th>Document</th>
      <th>Page map</th>
      <th>Last modified</th>
      <th>Updated by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentation.each do |documentation| %>
      <tr>
        <td><%= documentation.documentation_object %></td>
        <td><%= documentation.document %></td>
        <td><%= documentation.page_map %></td>
        <td><%= documentation.updated_at %></td>
        <td><%= object_tag(documentation.updater) %></td>
        <td><%= link_to 'Show', documentation %></td>
        <td><%= link_to 'Edit', edit_documentation_path(documentation) %></td>
        <td><%= link_to 'Destroy', documentation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
