<h1>Listing identifiers</h1>

<table>
  <thead>
    <tr>
      <th>Identified object</th>
      <th>Identified object type</th>
      <th>Identifier</th>
      <th>Type</th>
      <th>Cached identifier</th>
      <th>Namespace</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @identifiers.each do |identifier| %>
      <tr>
        <td><%= identifier.identified_object_id %></td>
        <td><%= identifier.identified_object_type %></td>
        <td><%= identifier.identifier %></td>
        <td><%= identifier.type %></td>
        <td><%= identifier.cached_identifier %></td>
        <td><%= identifier.namespace_id %></td>
        <td><%= identifier.created_by_id %></td>
        <td><%= identifier.updated_by_id %></td>
        <td><%= link_to 'Show', identifier %></td>
        <td><%= link_to 'Edit', edit_identifier_path(identifier) %></td>
        <td><%= link_to 'Destroy', identifier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identifier', new_identifier_path %>
