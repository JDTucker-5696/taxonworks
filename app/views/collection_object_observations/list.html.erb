
<h1>Listing collection object observations</h1>
<%= paginate @collection_object_observations %> <%= page_entries_info @collection_object_observations %>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Last modified</th>
      <th>Updated by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collection_object_observations.each do |collection_object_observation| %>
      <tr>
        <td><%= collection_object_observation.data %></td>
        <td><%= collection_object_observation.updated_at %></td>
        <td><%= object_tag(collection_object_observation.updater) %></td>
        <td><%= link_to 'Show', collection_object_observation %></td>
        <td><%= link_to 'Edit', edit_collection_object_observation_path(collection_object_observation) %></td>
        <td><%= link_to 'Destroy', collection_object_observation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

