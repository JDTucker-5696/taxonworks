$('[data-pin-button-item-id="<%= @pinboard_item.pinned_object_id -%>"]').replaceWith("<%= j pin_item_to_pinboard_link(@pinboard_item.pinned_object, sessions_current_user) -%>");
$('[data-pin-item="<%= @pinboard_item.id -%>"]').remove();
if(!$('[data-pin-caterogy="<%= @pinboard_item.pinned_object_type -%>"] li').length) {
	$('[data-pin-caterogy="<%= @pinboard_item.pinned_object_type -%>"]').parent().remove();
}
$('body').mx_spinner('hide');
TW.workbench.alert.create("Pinboard item was successfully destroyed.", "notice");