<h1>Listing Otus</h1>

<%= paginate @otus %> <%= page_entries_info @otus %>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Last modified</th>
    <th>Updated by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @otus.each do |otu| %>
      <tr>
        <td><%= otu.name %></td>
        <td><%= otu.updated_at %></td>
        <td><%= object_tag(asserted_distribution.updater) %></td>
        <td><%= link_to 'Show', otu %></td>
        <td><%= link_to 'Edit', edit_otu_path(otu) %></td>
        <td><%= link_to 'Destroy', otu, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

