{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "plugins": [
    "vue",
    "import"
  ],
  "env": {
    "browser": "true",
    "es6": "true",
    "jquery": "true"
  },
  "globals": {
    "TW": "true",
    "jquery": "true",
    "defineEmits": true,
    "defineProps": true
  }
}